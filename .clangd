CompileFlags:
  Add:
    - -std=gnu2x
    - -xc
    - -Wall
    - -Wextra
    - -Wpedantic
    - -Wconversion
    - -Wtype-limits
    - -Werror=return-type
    - -Werror=incompatible-pointer-types
    - -Wno-varargs
    - -Wno-gnu-zero-variadic-macro-arguments
    - -Wno-gnu-statement-expression-from-macro-expansion
    - -Wvla
    - -Wno-format-pedantic
Diagnostics:
  Suppress: ["unused-includes"]
